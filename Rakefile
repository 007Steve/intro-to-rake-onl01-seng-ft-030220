namespace :greeting do 
  desc 'outputs hello to the terminal'
  task :hello do 
    puts"hello from Rake!"
  end



desc 'outputs hello to the terminal'
task :hola do
  puts "hola de Rake!"
end

desc 'outputs console  to the terminal'
task :console do
  puts "Make sure you have a 'console' rake task"
end

namespace :db do 
  desc'migrate changes to your database'
  task :migrate => :
end
